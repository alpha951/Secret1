<%- include('partials/header') %>
  <div class="jumbotron text-center">
    <div class="container">
      <i class="fas fa-key fa-6x"></i>
      <h1 class="display-3">You've Discovered My Secret!</h1>
      <%users.forEach(function(user){%>
        <% user.secret.forEach(function(manysecrets){ %>
          <p class="secret-text">
            <%=manysecrets%>
          </p>
          <% }); %>
            <% }) %>
              <hr>
              <%if(isAuthenticated){%>
                <a class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
                <a class="btn btn-dark btn-lg" href="/submit" role="button">Submit a Secret</a>
                <%}else{%>
                  <a class="btn btn-dark btn-lg" href="/" role="button">Register or login to submit a secret!</a>
                  <% }%>
    </div>
  </div>
  <%- include('partials/footer') %>